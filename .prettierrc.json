{
  "singleQuote": false,
  "semi": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "arrowParens": "always",
  "endOfLine": "lf",
  "proseWrap": "always",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^react$",
    "^react-native$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@niveshstar/",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"]
}
